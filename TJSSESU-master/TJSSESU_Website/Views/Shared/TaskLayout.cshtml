    <body style="background: #fafafa">
        <div uk-sticky="top: 300; animation: uk-animation-slide-top" class="uk-navbar-container tm-navbar-container uk-sticky uk-sticky-fixed">
            <div class="uk-container uk-container-expand uk-background-secondary">
                <nav class="uk-navbar">
                    <div class="uk-navbar-left">
                        <a href="#" class="uk-navbar-item uk-logo uk-visible@s" style="color: white; strong;">TJSSESU</a>
                    </div>
                    <div class="uk-navbar-left">
                        <div class="uk-navbar-item uk-inline">
                            <a href="#" class="uk-navbar-item uk-margin-small-right uk-hidden@s" style="color: white;" uk-icon="icon:menu;ratio:2"></a>
                            <div uk-dropdown="mode: hover">
                                <ul class="uk-nav uk-dropdown-nav" style="font-size: 20px;line-height: 2;">
                                    <div class="uk-inline">
                                        <li><a href="#" uk-scroll>任务</a></li>
                                        <div uk-dropdown="mode: hover">
                                            <li><a href="#tasks" uk-scroll>我发布的任务</a></li>
                                            <li><a href="#tasks" uk-scroll>我接受的任务</a></li>
                                        </div>
                                    </div>
                                    <li><a href="#file" uk-scroll>文件</a></li>
                                    <li><a href="#info" uk-scroll>个人信息</a></li>
                                </ul>
                            </div>
                        </div>

                    </div>
